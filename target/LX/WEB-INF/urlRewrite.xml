<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.ory//DTD UrlRewrite 3.2//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.2.dtd">
<urlrewrite>
    <rule>
        <from>/userinf-(\w*).html</from>
        <to type="forward">/userinf.jsp?username=$1</to>
    </rule>
</urlrewrite>